<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments.Administrator.Category.EditCategoryFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/editCategoryTitle"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="@string/editCategoryTitle"
            android:textColor="@color/DarkPurple"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/guideline167"
            app:layout_constraintEnd_toStartOf="@+id/guideline165"
            app:layout_constraintStart_toStartOf="@+id/guideline160"
            app:layout_constraintTop_toTopOf="@+id/guideline158" />

        <Button
            android:id="@+id/editCategoryCancelBtn"
            style="@style/btnBackTheme"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="@string/editCategoryCancelBtn"
            app:layout_constraintBottom_toTopOf="@+id/guideline159"
            app:layout_constraintEnd_toStartOf="@+id/guideline161"
            app:layout_constraintStart_toStartOf="@+id/guideline160"
            app:layout_constraintTop_toTopOf="@+id/guideline166" />

        <Button
            android:id="@+id/editCategoryEditBtn"
            style="@style/btnGoTheme"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="@string/editCategoryEditBtn"
            app:layout_constraintBottom_toTopOf="@+id/guideline159"
            app:layout_constraintEnd_toStartOf="@+id/guideline165"
            app:layout_constraintStart_toStartOf="@+id/guideline164"
            app:layout_constraintTop_toTopOf="@+id/guideline166" />

        <Button
            android:id="@+id/editCategoryDeleteBtn"
            style="@style/btnGoTheme"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="4dp"
            android:text="@string/editCategoryDeleteBtn"
            app:layout_constraintBottom_toTopOf="@+id/guideline159"
            app:layout_constraintEnd_toStartOf="@+id/guideline163"
            app:layout_constraintStart_toStartOf="@+id/guideline162"
            app:layout_constraintTop_toTopOf="@+id/guideline166" />

        <ImageView
            android:id="@+id/editCategoryImage"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline169"
            app:layout_constraintEnd_toStartOf="@+id/guideline164"
            app:layout_constraintStart_toStartOf="@+id/guideline161"
            app:layout_constraintTop_toTopOf="@+id/guideline168"
            tools:src="@tools:sample/avatars" />

        <TextView
            android:id="@+id/editCategoryName"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="@string/editCategoryName"
            android:textColor="@color/DarkPurple"
            app:layout_constraintBottom_toTopOf="@+id/guideline173"
            app:layout_constraintEnd_toStartOf="@+id/guideline161"
            app:layout_constraintStart_toStartOf="@+id/guideline132"
            app:layout_constraintTop_toTopOf="@+id/guideline171" />

        <EditText
            android:id="@+id/editCategoryEditName"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="@string/editCategoryEditName"
            app:layout_constraintBottom_toTopOf="@+id/guideline172"
            app:layout_constraintEnd_toStartOf="@+id/guideline165"
            app:layout_constraintStart_toStartOf="@+id/guideline161"
            app:layout_constraintTop_toTopOf="@+id/guideline170"
            app:layout_constraintVertical_bias="0.506" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline132"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.05" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline133"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.95" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline158"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.05" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline159"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.95" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline160"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.1" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline161"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.3" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline162"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.40" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline163"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.60" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline164"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.70" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline165"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.90" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline166"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.88" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline167"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.20" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline168"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.25" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline169"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.45" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline170"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.60" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline171"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.65" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline172"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.70" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline173"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.75" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>