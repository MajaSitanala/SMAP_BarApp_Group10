            android:textSize="30sp"
            app:layout_constraintBottom_toTopOf="@+id/guideline88"
            app:layout_constraintEnd_toStartOf="@+id/guideline107"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/guideline103"
            app:layout_constraintTop_toTopOf="@+id/guideline86"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/addTutorNickName"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="@string/addTutorNickName"
            app:layout_constraintBottom_toTopOf="@+id/guideline97"
            app:layout_constraintEnd_toStartOf="@+id/guideline89"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/guideline73"
            app:layout_constraintTop_toTopOf="@+id/guideline108" />

        <TextView
            android:id="@+id/addTutorRealName"
            android:layout_width="0dp"
