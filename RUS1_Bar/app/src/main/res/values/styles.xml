    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <!--<item name="colorPrimary">@color/colorPrimary</item> -->
        <item name="colorPrimary">@color/LightPurple</item>
        <!--<item name="colorPrimaryDark">@color/colorPrimaryDark</item> -->
        <item name="colorPrimaryDark">@color/DarkPurple</item>
       <!--<item name="colorAccent">@color/colorAccent</item> -->
        <item name="colorAccent">@color/LightPurple</item>
    </style>
    <style name="btnBackTheme" parent="android:Widget.Button">
        <item name="android:textColor">@color/Grey</item>
        <item name="android:background">@color/DarkPurple</item>
