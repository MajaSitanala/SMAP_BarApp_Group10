    <style name="btnGoTheme" parent="android:Widget.Button">
        <item name="android:textColor">@color/LightPurple</item>
        <item name="android:background">@color/Grey</item>
    </style>
    <style name="TxtVwtheme" parent="android:Widget.TextView">
        <item name="android:background">@color/Grey</item>
        <item name="android:textColor">@color/LightPurple</item>
    </style>
